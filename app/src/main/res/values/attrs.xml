<resources>

    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="MainEditText">
        <attr name="mainEditTextHint" format="string" />
        <attr name="mainEditTextHelper" format="string" />
        <attr name="mainEditTextType" format="enum">
            <enum name="name" value="0"/>
            <enum name="address" value="1"/>
            <enum name="latitude" value="2"/>
            <enum name="longitude" value="3"/>
            <enum name="distance" value="4"/>
        </attr>
    </declare-styleable>

    <attr name="primaryTextColor" format="color" />
</resources>
